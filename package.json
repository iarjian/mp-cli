{
  "name": "@ruochuan/mp-cli",
  "version": "0.9.1",
  "main": "src/index.js",
  "type": "module",
  "keywords": [
    "nodejs",
    "node",
    "miniprogram-ci",
    "miniprogram",
    "ci",
    "cli",
    "mp-cli"
  ],
  "bin": {
    "mp-cli": "src/index.js",
    "rmc": "src/index.js",
    "ruochuan-mp-cli": "src/index.js"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "repository": "https://github.com/lxchuan12/mp-cli.git",
  "license": "MIT",
  "scripts": {
    "test": "echo test",
    "lint": "eslint --cache --fix --ext .js",
    "ci": "node src/index.js",
    "upload": "npm run ci -- --upload",
